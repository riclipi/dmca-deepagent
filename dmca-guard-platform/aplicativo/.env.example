# Database
DATABASE_URL="postgresql://username:password@host:port/database"

# NextAuth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your-nextauth-secret-here"

# Google OAuth
GOOGLE_CLIENT_ID="your-google-client-id-here"
GOOGLE_CLIENT_SECRET="your-google-client-secret-here"

# Email
RESEND_API_KEY="your-resend-api-key-here"
RESEND_SENDER_FROM_EMAIL="guard@yourdomain.com"
RESEND_DOMAIN="yourdomain.com"
RESEND_SENDER_NAME="DMCA Guard"

SUPER_USER_EMAIL="admin@yourdomain.com"

# Environment
NODE_ENV="development"

# Search APIs para busca real (CONFIGURE ESTAS!)
# Serper API (https://serper.dev/) - OBRIGATÃ“RIA para testar busca real
SERPER_API_KEY="your-serper-api-key-here"

# Google Custom Search (alternativa gratuita ao Serper)
# Obtenha em: https://developers.google.com/custom-search/v1/introduction
GOOGLE_API_KEY="your-google-api-key-here"
GOOGLE_CSE_ID="your-google-cse-id-here"

# Modo de teste (true = gera resultados simulados, false = usa APIs reais)
SEARCH_TEST_MODE="false"
